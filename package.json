{
  "name": "dantefy",
  "version": "0.0.1",
  "description": "a bot that posts on both twitter and mastodon.",
  "private": true,
  "main": "dist/main.js",
  "scripts": {
    "start": "node dist/main.js",
    "twoot-one": "node dist/main.js once",
    "build": "rm -rf dist/ && tsc",
    "test": "tsc --noEmit && eslint 'src/**' && prettier 'src/**' -l",
    "watch": "tsc -w",
    "dev": "ts-node-dev --respawn --transpileOnly --notify=false src/main.ts -- local"
  },
  "author": "s <https://github.com/lostfictions>",
  "license": "AGPL-3.0",
  "dependencies": {
    "axios": "^0.18.0",
    "canvas": "^2.5.0",
    "envalid": "^4.1.2",
    "node-schedule": "^1.3.0",
    "source-map-support": "^0.5.8",
    "twoot": "0.0.3"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.8",
    "@types/node": "^10.7.1",
    "@types/node-schedule": "^1.2.2",
    "eslint": "^5.3.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-typescript": "^0.12.0",
    "prettier": "^1.14.2",
    "ts-node-dev": "^1.0.0-pre.26",
    "typescript": "^3.0.1",
    "typescript-eslint-parser": "^18.0.0"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
